  <refsection>
    <title>Options</title>

    <variablelist>
      <title>Input/Output Options</title>

      <varlistentry>
        <term><option>-i <replaceable>file</replaceable></option></term>
        <listitem>
          <para>
	    Flatten the asfix parse tree in
	    <replaceable>file</replaceable>. 
	  </para>
        </listitem>
      </varlistentry>

      <varlistentry>
        <term><option>-o <filename><replaceable>file</replaceable></filename></option></term>
        <listitem>
	  <para>
	    Write the resulting text to
	    <filename><replaceable>file</replaceable></filename>.
	  </para>

	  <para>
	    The resulting file corresponds exactly to the text file
	    that the asfix file was produced from.
	  </para>
        </listitem>
      </varlistentry>
    </variablelist>

